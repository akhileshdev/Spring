<?xml version="1.0" encoding="UTF-8"?>

<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="ques" class="com.spring.practise.Question">  

<constructor-arg value = "101"/>
<constructor-arg value = "what is Spring ?"/>

<constructor-arg>
<list>

<value>Spring is Backend Framework</value>
<value>Java based framework</value>
<value>Most popular Java based server side framework</value>

</list>
</constructor-arg>
  
</bean>  
  
</beans>  