<?xml version="1.0" encoding="UTF-8"?>

<beans                            
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
                        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
                   


<bean id = "questionbean" class = "com.java.spring.ci.collection.Question">
  <constructor-arg value = "101"></constructor-arg>
  <constructor-arg value = "What is Java ?"></constructor-arg>
  <constructor-arg>
     
     <list>
        <value>It is a programming language</value>
        <value>It is an OOPS Paradigm</value>
        <value>It is not pure Object Oriented Programming language</value>
     </list>
 
  </constructor-arg>
</bean>


</beans>